<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>HelloWorld</display-name>

	<welcome-file-list>

		<welcome-file>index.html</welcome-file>

	</welcome-file-list>

	<servlet>
		<servlet-name>hw</servlet-name>
		<servlet-class>com.oaec.test.Test1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ho</servlet-name>
		<servlet-class>com.oaec.test.helloworld</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ho</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>hw</servlet-name>
		<url-pattern>/Test1</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>registr</servlet-name>
		<servlet-class>com.oaec.test.Register</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registr</servlet-name>
		<url-pattern>/Test1</url-pattern>
	</servlet-mapping>





</web-app>